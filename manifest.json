{
  "manifest_version": 3,
  "name": "__MSG_extName__",
  "version": "1.0.4",
  "description": "__MSG_extDescription__",
  "default_locale": "en",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "file:///*/*"
      ],
      "js": [
        "libs/marked.min.js",
        "libs/highlight.min.js",
        "libs/purify.min.js",
        "libs/mermaid.min.js",
        "libs/katex.min.js",
        "libs/auto-render.min.js",
        "libs/beautify-html.min.js",
        "content.js"
      ],
      "css": [
        "libs/highlight-github.min.css"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "libs/katex.min.css",
        "libs/fonts/*"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}
